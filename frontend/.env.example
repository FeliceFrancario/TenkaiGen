# Copy this file to .env.local and fill in your values
# OpenRouter credentials
OPENROUTER_API_KEY=sk-or-...
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL=google/gemma-3-12b-it:free
OPENROUTER_SITE_URL=http://localhost:3000
OPENROUTER_SITE_NAME=TenkaiGen

# Gemini (OpenAI SDK over Google endpoint)
GEMINI_API_KEY=your_google_api_key
GEMINI_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai
# e.g., gemini-2.0-flash (default). You can try gemini-1.5-flash or others if available to your key.
GEMINI_MODEL=gemini-2.0-flash
